{
    "name": "gotxiko-cli",
    "version": "1.0.2",
    "description": "A small CLI tool for various useful tasks including SSL certificate checking, curl info, and git user switching",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "type": "module",
    "bin": {
        "gcli": "./dist/index.js"
    },
    "files": [
        "dist/**/*",
        "scripts/**/*",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "bun build index.ts --outdir dist --target bun",
        "format": "bunx prettier . --write",
        "prepublishOnly": "bun run build"
    },
    "keywords": [
        "cli",
        "ssl",
        "curl",
        "git",
        "certificate",
        "tool"
    ],
    "author": "Gotxiko",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Gotxiko/gotxiko-cli.git"
    },
    "homepage": "https://github.com/Gotxiko/gotxiko-cli#readme",
    "bugs": {
        "url": "https://github.com/Gotxiko/gotxiko-cli/issues"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "prettier": "3.6.2"
    },
    "peerDependencies": {
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "@types/yargs": "^17.0.34",
        "yargs": "^18.0.0"
    }
}
